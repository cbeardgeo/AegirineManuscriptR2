publisher = {Elsevier B.V.},
title = {{Effects of fluorine on the solubilities of Nb, Ta, Zr and Hf minerals in highly fluxed water-saturated haplogranitic melts}},
url = {http://dx.doi.org/10.1016/j.oregeorev.2014.02.014},
