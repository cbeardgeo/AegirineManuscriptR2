doi = {10.1016/0375-6742(86)90007-5},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Cole, Drummond/1986 - Cole, Drummond - The effect of transport and boiling on AgAu ratios in hydrothermal solutions a preliminary assessment and possib.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Cole, Drummond/1986 - Cole, Drummond - The effect of transport and boiling on AgAu ratios in hydrothermal solutions a preliminary assessment and pos(2).pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Cole, Drummond/1986 - Cole, Drummond - The effect of transport and boiling on AgAu ratios in hydrothermal solutions a preliminary assessment and possi.html:html},
