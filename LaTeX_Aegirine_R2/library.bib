number = {2},
pages = {83--102},
title = {{Influence of the X-site composition on tourmaline's crystal structure: investigation of synthetic K‐dravite, dravite, oxy‐uvite, and magnesio‐foitite using SREF and Raman spectroscopy}},
url = {http://link.springer.com/article/10.1007/s00269-015-0776-3 http://link.springer.com/content/pdf/10.1007{\%}2Fs00269-015-0776-3.pdf},
