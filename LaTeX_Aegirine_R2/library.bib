doi = {10.1016/j.epsl.2010.07.012},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Blichert-Toft, Puchtel/2010 - Blichert-Toft, Puchtel - Depleted mantle sources through time Evidence from Lu–Hf and Sm–Nd isotope systematics of Archean komati.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Blichert-Toft, Puchtel/2010 - Blichert-Toft, Puchtel - Depleted mantle sources through time Evidence from Lu–Hf and Sm–Nd isotope systematics of Archean komat.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Blichert-Toft, Puchtel/2010 - Blichert-Toft, Puchtel - Depleted mantle sources through time Evidence from Lu–Hf and Sm–Nd isotope systematics of Archean ko(2).html:html},
