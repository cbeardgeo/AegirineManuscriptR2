issn = {0022-3530, 1460-2415},
journal = {Journal of Petrology},
keywords = {Basalts,Folder - Peralkaline Field Area - Tenerife,clinopyroxenes,crustal structure,fractional crystallization,magma mixing,trace elements},
language = {en},
mendeley-tags = {Basalts,Folder - Peralkaline Field Area - Tenerife,clinopyroxenes,crustal structure,fractional crystallization,magma mixing,trace elements},
