month = {sep},
number = {5},
pages = {417--427},
shorttitle = {Agpaitic nepheline syenites},
title = {{Agpaitic nepheline syenites: a potential source of rare elements}},
url = {http://www.sciencedirect.com/science/article/pii/088329279290003L http://www.sciencedirect.com/science/article/pii/088329279290003L/pdf?md5=aceff1dd73513d1dc23057f517d32280{\&}pid=1-s2.0-088329279290003L-main.pdf},
