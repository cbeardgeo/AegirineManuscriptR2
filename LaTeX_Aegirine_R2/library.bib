file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Meneghini, Fagereng, Kisters/2017 - Meneghini, Fagereng, Kisters - The matchless amphibolite of the Damara Belt, Namibia Unique preservation of a late neoproterozoic.pdf:pdf},
issn = {03912612},
journal = {Ofioliti},
keywords = {Late Neo-Proterozoic Damara Belt. Namibia,Ophiolitic suture,Ridge subduction},
number = {2},
pages = {129--145},
title = {{The matchless amphibolite of the Damara Belt, Namibia: Unique preservation of a late neoproterozoic ophiolitic suture}},
volume = {42},
year = {2017}
}
