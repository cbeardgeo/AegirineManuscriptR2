@article{Burke2006,
abstract = {The association of alkaline igneous rocks and carbonatites (ARCs) with$\backslash$nintracontinental rifts has long been recognized. Deformed alkaline rocks$\backslash$n(such as nepheline syenites) and carbonatites (DARCs), which form a$\backslash$nsmall subset ({\textless}10{\%}) of ARCs and carbonatites, have become a focus of$\backslash$nattention since it was recognized that in some places they are$\backslash$nassociated with suture zones marking locations where oceans have closed.$\backslash$nThe association of DARCs with sutures can be readily understood in terms$\backslash$nof the operation of the Wilson cycle in the opening and the closing of$\backslash$nocean basins. The Wilson cycle hypothesis for the origin of DARCs has$\backslash$nbeen tested for the Proterozoic of both Africa and India. For Africa, an$\backslash$nadditional hypothesis that we have also tested is that ARCs in$\backslash$nintracontinental rifts represent magmas made by partial melting of DARC$\backslash$nmaterial in the underlying mantle lithosphere. A limitation of the$\backslash$nAfrican and Indian studies is that they are local and regional. Only a$\backslash$nworldwide geoinformatic study can fully test the two hypotheses. If one$\backslash$nof the hypothesis can be invalidated that will have profound$\backslash$npetrogenetic significance. In this study, we describe our ongoing$\backslash$ngeoinformatic work on the worldwide distribution of ARCs and DARCs,$\backslash$nincluding the databases, techniques, and operating procedures. To$\backslash$nillustrate our approach, we report on a local analysis that we have$\backslash$ncarried out on the ARCs and DARCs of the Kola Peninsula in Arctic$\backslash$nRussia.},
author = {Burke, Kevin and Khan, Shuhab},
doi = {10.1130/GES00027.1},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Burke, Khan/2006 - Burke, Khan - Geoinformatic approach to global nepheline syenite and carbonatite distribution Testing a Wilson cycle model.pdf:pdf},
isbn = {1553-040X},
issn = {1553040X},
journal = {Geosphere},
keywords = {Carbonatites,Deformed alkaline rocks,Geoinformatics,Kola peninsula.{\textless}/kwdg{\textgreater}},
number = {1},
pages = {53--60},
title = {{Geoinformatic approach to global nepheline syenite and carbonatite distribution: Testing a Wilson cycle model}},
volume = {2},
year = {2006}
}
@inproceedings{Boudreau2008a,
abstract = {The Merensky pegmatoid (normal reef) in the western Bushveld Complex is commonly characterized as a pyroxene-rich pegmatoidal unit with a base that is enriched in chromite and platinum-group element-bearing sulfides overlying a leuconorite footwall. Models for its formation have ranged from those that view it as entirely a magmatic cumulate succession to those that have suggested that it is a zone of volatile-induced remelting. The consequences of the latter interpretation are investigated using the numerical modeling program IRIDIUM, which links diffusive and advective mass and heat transport with a phase equilibration routine based on the MELTS program. The initial system consists of a simple stratigraphic succession of a partially molten leuconorite overlain by a partially molten pyroxenite, both initially at 1,190°C and 2 kbar. 2 wt{\%} of a volatile fluid composed of 75 mol{\%} H2O, 20 mol{\%} CO2 and 5 mol{\%} H2S is then added to the lower 20 cm of the pyroxenite. The system is then allowed to evolve under conditions of chemical diffusion in the liquid. The addition of the volatile components results in a modest increase in the amount of melt in the pyroxenite. However, chemical diffusion across the leuconorite–pyroxenite boundary leads to more extensive melting at and below the boundary with preferential loss of opx from the underlying leuconorite, preferential re-precipitation of sulfide and chromite and concentration of the PGE at this boundary. These results mimic actual mineral and compositional profiles across the Merensky pegmatoid and illustrate that long-term diffusion process can effectively produce mineralogical and compositional layering not present in the original assemblage.},
author = {Hoshino, M. and Watanabe, Y. and Murakami, H. and Kon, Y. and Tsunematsu, M.},
booktitle = {Contributions to Mineralogy and Petrology},
doi = {10.1007/s00410-008-0294-0},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Hoshino et al/2012 - Hoshino et al. - Crystallization process of zircon and fergusonite during hydrothermal alteration in Nechalacho REE deposit, (2).html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Hoshino et al/2012 - Hoshino et al. - Crystallization process of zircon and fergusonite during hydrothermal alteration in Nechalacho REE deposit, Thor.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Hoshino et al/2012 - Hoshino et al. - Crystallization process of zircon and fergusonite during hydrothermal alteration in Nechalacho REE deposit, Tho.html:html},
issn = {0010-7999, 1432-0967},
