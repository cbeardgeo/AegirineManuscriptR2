pages = {161--175},
title = {{Experimental effects of pressure and fluorine on apatite saturation in mafic magmas, with reference to layered intrusions and massif anorthosites}},
url = {http://link.springer.com/article/10.1007/s00410-008-0279-z http://link.springer.com/article/10.1007/s00410-008-0279-z{\#}page-1 http://link.springer.com/content/pdf/10.1007/s00410-008-0279-z.pdf},
