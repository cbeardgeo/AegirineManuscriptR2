doi = {10.1016/0009-2541(90)90122-N},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Dupr{\'{e}}, Arndt/1990 - Dupr{\'{e}}, Arndt - Pb Isotopic Compositions of Archean Komatiites and Sulphides.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Dupr{\'{e}}, Arndt/1990 - Dupr{\'{e}}, Arndt - Pb Isotopic Compositions of Archean Komatiites and Sulphides.html:html},
