@article{Sorrensen1992,
abstract = {The agpaitic nepheline syenites may, with regard to their elevated contents of a number of rare elements, be compared with the complex granite pegmatites. Both rock types form highly evolved magmas under closed system conditions which prevent volatiles from escaping. Agpaitic rocks are peralkaline nepheline syenites characterized by the presence of complex rare-element minerals such as eudialyte and mosandrite, instead of the simple minerals such as zircon and ilmenite. Uranium deposits have been found in agpaitic rocks in Brazil (Po??os de Caldas), China (Saima) and Greenland (Il??maussaq), Nb and apetite deposits in Russia (Lovozero and Khibina). The type locality of agpaitic rocks, the Il??maussaq complex in south Greenland, is characterized by elevated contents of elements such as Zr, REE, Y, Be, Li, Nb, U, Th and F, not only in pegmatites, late veins and contact fenites, but also in some of the rocks, especially lujavrite and kakortokite. The most important rare-element containing minerals are eudialyte (Zr, REE, Y, Nb), steenstrupine (U, Th, REE), rinkite (Nb, REE, Y, U), pyrochlore and perovskite (Nb, Ta), Li-mica (Li), chkalovite (Be) and villiaumite (F). Occurrences of eudialyte-rich kakortokite and lujavrite are presently being explored in the Il??maussaq complex as sources of Zr, REE + Y and Nb. ?? 1992.},
author = {S{\o}rrensen, Henning},
doi = {10.1016/0883-2927(92)90003-L},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/S{\o}rrensen/1992 - S{\o}rrensen - Agpaitic nepheline syenites a potential source of rare elements.pdf:pdf},
isbn = {0883-2927},
issn = {08832927},
journal = {Applied Geochemistry},
number = {5},
pages = {417--427},
title = {{Agpaitic nepheline syenites: a potential source of rare elements}},
volume = {7},
year = {1992}
}
@article{Kitney2011a,
abstract = {Trachybasalt scoria from a cinder cone near the Mexican volcanic front contain phenocrysts of olivine with chromite inclusions, apatite, augite and hornblende, with microphenocrysts of plagioclase. The water-saturated phase relations reproduce the phenocryst assemblage between 1040°C and 970°C with water contents of between 2{\textperiodcentered}5 and 4{\textperiodcentered}5{\%} (50–150 MPa). The absence of biotite phenocrysts in the scoria places a tight constraint on the pressure–temperature conditions of phenocryst equilibration, as there is only a small zone where biotite does not accompany hornblende in the experiments. Diluting the fluid phase with CO2 changes the composition of the olivine, indicating that CO2 was only a minor component of the fluid of the scoria. Hornblende is stable to 1040°C at oxygen fugacities of NNO + 2 (where NNO is the nickel–nickel oxide buffer), but at lower oxygen fugacities, the upper limit is 990°C. There is a progressive increase in crystallinity in experimental runs as both pressure and temperature decrease. Isobaric plots of crystallinity show that the onset of hornblende crystallization involves a reaction relation, and also results in a marked ∼15–40 vol. {\%} increase in crystallinity. Ascending hydrous magmas intersecting the cooler crust could be trapped there by the large increase in crystallinity accompanying the isobaric crystallization of hornblende.},
