pages = {287--308},
title = {{Experimental rare earth element partition coefficients for garnet, clinopyroxene and amphibole coexisting with andesitic and basaltic liquids}},
url = {http://www.sciencedirect.com/science/article/pii/0016703780901386 http://www.sciencedirect.com/science/article/pii/0016703780901386/pdf?md5=0096ba5cfc64dee54794ebec49a37127{\&}pid=1-s2.0-0016703780901386-main.pdf},
volume = {44},
