isbn = {0024-4937},
issn = {18726143},
journal = {Lithos},
keywords = {Apatite,Carbonatites,Element partitioning,Phoscorites,Raman micro-spectroscopy,Trace-element composition},
pages = {188--213},
title = {{Apatite in carbonatitic rocks: Compositional variation, zoning, element partitioning and petrogenetic significance}},
volume = {274-275},
year = {2017}
}
