@article{Halama2005,
abstract = {The Gr{\o}nnedal-Ika complex is dominated by layered nepheline syenites which were intruded by a xenolithic syenite and a central plug of calcite to calcite-siderite carbonatite. Aegirine-augite, alkali feldspar and nepheline are the major mineral phases in the syenites, along with rare calcite. Temperatures of 680-910°C and silica activities of 0{\textperiodcentered}28-0{\textperiodcentered}43 were determined for the crystallization of the syenites on the basis of mineral equilibria. Oxygen fugacities, estimated using titanomagnetite compositions, were between 2 and 5 log units above the fayalite-magnetite-quartz buffer during the magmatic stage. Chondrite-normalized REE patterns of magmatic calcite in both carbonatites and syenites are characterized by REE enrichment (LaCN-YbCN = 10-70). Calcite from the carbonatites has higher Ba (∼5490 ppm) and lower HREE concentrations than calcite from the syenites (54-106 ppm Ba). This is consistent with the behavior of these elements during separation of immiscible silicate-carbonate liquid pairs. $\epsilon$Nd(T = 1{\textperiodcentered}30 Ga) values of clinopyroxenes from the syenites vary between +1{\textperiodcentered}8 and +2{\textperiodcentered}8, and $\epsilon$Nd(T) values of whole-rock carbonatites range from +2{\textperiodcentered}4 to +2{\textperiodcentered}8. Calcite from the carbonatites has $\delta$18O values of 7{\textperiodcentered}8 to 8{\textperiodcentered} 6‰ and $\delta$13C values of -3{\textperiodcentered}9 to - 4{\textperiodcentered} 6‰. $\delta$18O values of clinopyroxene separates from the nepheline syenites range between 4{\textperiodcentered}2 and 4{\textperiodcentered}9‰. The average oxygen isotopic composition of the nepheline syenitic melt was calculated based on known rock-water and mineral-water isotope fractionation to be 5{\textperiodcentered}7 ± 0{\textperiodcentered}4‰. Nd and C-O isotope compositions are typical for mantle-derived rocks and do not indicate significant crustal assimilation for either syenite or carbonatite magmas. The difference in $\delta$18O between calculated syenitic melts and carbonatites, and the overlap in $\epsilon$ Nd values between carbonatites and syenites, are consistent with derivation of the carbonatites from the syenites via liquid immiscibility. {\textcopyright} Oxford University Press 2004; all rights reserved.},
author = {Halama, Ralf and Vennemann, Torsten and Siebel, Wolfgang and Markl, Gregor},
doi = {10.1093/petrology/egh069},
issn = {00223530},
journal = {Journal of Petrology},
keywords = {Alkaline magmatism,Carbonatite,Gardar Province,Liquid immiscibility,Nepheline syenite},
number = {1},
pages = {191--217},
title = {{The Gr??nnedal-Ika carbonatite - Syenite complex, south Greenland: Carbonatite formation by liquid immiscibility}},
volume = {46},
year = {2005}
}
