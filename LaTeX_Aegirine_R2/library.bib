month = {feb},
number = {3},
pages = {337--356},
shorttitle = {Clinopyroxene/liquid trace element partitioning in},
title = {{Clinopyroxene/liquid trace element partitioning in natural trachyte--trachyphonolite systems: insights from Campi Flegrei (southern Italy)}},
url = {http://link.springer.com/article/10.1007/s00410-009-0386-5 http://link.springer.com/article/10.1007/s00410-009-0386-5{\#}page-1 http://link.springer.com/content/pdf/10.1007{\%}2Fs00410-009-0386-5.pdf},
volume = {158},
