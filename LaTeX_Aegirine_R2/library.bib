doi = {10.1029/JB089iB10p08616},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Rytuba, McKee/1984 - Rytuba, McKee - Peralkaline ash flow tuffs and calderas of the McDermitt Volcanic Field, southeast Oregon and north central Nevad.pdf:pdf},
