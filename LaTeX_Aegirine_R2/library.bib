shorttitle = {Energy-Constrained Open-System Magmatic Processes },
title = {{Energy-constrained open-system magmatic processes I: General model and energy-constrained assimilation and fractional crystallization (EC-AFC) formulation}},
url = {http://petrology.oxfordjournals.org/content/42/5/999 http://petrology.oxfordjournals.org/content/42/5/999.full.pdf http://petrology.oxfordjournals.org/content/42/5/999.short},
volume = {42},
