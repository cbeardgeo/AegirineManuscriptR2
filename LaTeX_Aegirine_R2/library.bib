month = {oct},
number = {1-2},
pages = {59--82},
shorttitle = {Silicic melts produced by reaction between peridot},
title = {{Silicic melts produced by reaction between peridotite and infiltrating basaltic melts: ion probe data on glasses and minerals in veined xenoliths from La Palma, Canary Islands}},
url = {http://link.springer.com/article/10.1007/s004100050582 http://link.springer.com/article/10.1007/s004100050582{\#}page-1 http://link.springer.com/content/pdf/10.1007{\%}2Fs004100050582.pdf},
