journal = {Central European Journal of Geosciences},
keywords = {Kangankunde,Malawi,RE mineralisation,Scanning Electron Microscopy,carbonatite,fluid-rock interaction,hydrothermal fluids,monazite,synchysite-(Ce)},
number = {4},
pages = {476--491},
title = {{Fluid-rock interaction in the Kangankunde Carbonatite Complex, Malawi: SEM based evidence for late stage pervasive hydrothermal mineralisation}},
