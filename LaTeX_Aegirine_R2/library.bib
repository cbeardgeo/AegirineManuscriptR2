isbn = {0010-7999},
issn = {00107999},
journal = {Contributions to Mineralogy and Petrology},
number = {4},
pages = {446--457},
title = {{Re-Os isotopic constrainst on magma mixing in the Peridotite Zone of the Stillwater Complex, Montana, USA}},
volume = {141},
year = {2001}
}
