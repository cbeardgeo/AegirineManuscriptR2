doi = {10.1086/467475},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Jung, Hoernes, Hoffer/2005 - Jung, Hoernes, Hoffer - Petrogenesis of Cogenetic Nepheline and Quartz Syenites and Granites (Northern Damara Orogen, Namibia) En.pdf:pdf},
issn = {0022-1376},
journal = {The Journal of Geology},
number = {6},
pages = {651--672},
title = {{Petrogenesis of Cogenetic Nepheline and Quartz Syenites and Granites (Northern Damara Orogen, Namibia): Enriched Mantle versus Crustal Contamination}},
url = {http://cat.inist.fr/?aModele=afficheN{\&}cpsidt=17249767{\%}5Cnhttp://www.journals.uchicago.edu/doi/10.1086/467475},
