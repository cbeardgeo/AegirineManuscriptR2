journal = {Journal of the Geological Society},
keywords = {cretaceous,direct,magmas are the principal,mantle,namibia,peridotite,source of information on,state of,the composition and physical,the subcontinental mantle,the widespread and abundant,xenoliths,xenoliths erupted in alkaline},
number = {3},
