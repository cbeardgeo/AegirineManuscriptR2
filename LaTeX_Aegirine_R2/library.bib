doi = {10.1007/BF00371485},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Pichavant/1981 - Pichavant - An experimental study of the effect of boron on a water saturated haplogranite at 1 Kbar vapour pressure.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Pichavant/1981 - Pichavant - An experimental study of the effect of boron on a water saturated haplogranite at 1 Kbar vapour pressure.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Linthout/1984 - Linthout - Alkali-zirconosilicates in peralkaline rocks.pdf:pdf},
