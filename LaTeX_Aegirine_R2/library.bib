file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Sillitoe, Burrows/2002 - Sillitoe, Burrows - New field evidence bearing on the origin of the El Laco magnetite deposit, Northern Chile.pdf:pdf},
isbn = {0361-0128},
issn = {03610128},
journal = {Economic Geology},
number = {5},
pages = {1101--1109},
pmid = {12021997},
