issn = {0012821X},
journal = {Earth and Planetary Science Letters},
keywords = {Comendites,Damaraland,Folder - Peralkaline Field Area - z.Other - Namibi,Magma mixing,Radiogenic isotopes,Ring complex,Syenites},
language = {en},
mendeley-tags = {Comendites,Damaraland,Folder - Peralkaline Field Area - z.Other - Namibi,Magma mixing,Radiogenic isotopes,Ring complex,Syenites},
