mendeley-tags = {LA ICP-MS U-Pb dating,Nechalacho Layered Suite,REE,Sm-Nd isotopes,nepheline syenite,stable isotopes},
month = {nov},
pages = {248--274},
publisher = {Elsevier B.V.},
title = {{Stable and radiogenic isotope constraints on the magmatic and hydrothermal evolution of the Nechalacho Layered Suite, northwest Canada}},
