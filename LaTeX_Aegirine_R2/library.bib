doi = {10.1080/08120099.2010.492911},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Hofmann/1988 - Hofmann - Chemical differentiation of the Earth the relationship between mantle, continental crust, and oceanic crust.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Bastow, Keir/2011 - Bastow, Keir - The protracted development of the continent-ocean transition in Afar.pdf:pdf},
