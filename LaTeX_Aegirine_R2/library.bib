@article{Hirschmann2006,
abstract = { Abstract Hydrous melting driven by changes in H2O storage capacity$\backslash$nmay occur in a variety of settings in the mantle, including in oceanic$\backslash$nbasalt sources and in deeper regions above and below the transition$\backslash$nzone. The 50вЂ“200 ppm H2O in the upper mantle likely derives from$\backslash$na blend of sources that may include residues of hydrous partial melting,$\backslash$neither in the deep mantle and/or beneath arcs or oceanic islands.$\backslash$nRelative to the large storage capacity in the transition zone, low$\backslash$nstorage capacities above and below may lead to hydrous melting for$\backslash$nmaterial upwelling through 410 km or downwelling through 670 km.$\backslash$nThe apparently very low storage capacity of the lower mantle ({\textless}20$\backslash$nppm H2O) may force melting even if downwelling rocks have normal$\backslash$nupper mantle H2O (50вЂ“200 ppm) concentrations. Very low storage$\backslash$ncapacity in the lower mantle, if verified experimentally, presents$\backslash$na challenge to the view that the H2O-rich sources of oceanic island$\backslash$nbasalts reside in the lower mantle. },
