shorttitle = {Zonation, mixing and eruption of silica-undersatur},
title = {{Borden dykes of Baffin Island, Northwest Territories: a Franklin U–Pb baddeleyite age and a paleomagnetic reinterpretation}},
url = {http://journals.cambridge.org/article{\_}S0016756800032039 http://journals.cambridge.org/action/displayAbstract?fromPage=online{\&}aid=4519160{\&}fileId=S0016756800032039},
