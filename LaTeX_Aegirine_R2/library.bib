journal = {Journal of African Earth Sciences},
keywords = {Alkaline complex,Ampasindava peninsula,Folder - Peralkaline Field Area - z.Other - Other,Madagascar,Peralkaline A-type granite,REE and HFSE mineralization,agpaitic},
mendeley-tags = {Alkaline complex,Ampasindava peninsula,Folder - Peralkaline Field Area - z.Other - Other,Madagascar,Peralkaline A-type granite,REE and HFSE mineralization,agpaitic},
number = {7},
pages = {1071--1098},
title = {{REE and HFSE mineralization in peralkaline granites of the Ambohimirahavavy alkaline complex, Ampasindava peninsula, Madagascar}},
