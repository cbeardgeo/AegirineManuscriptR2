issn = {1367-9120},
journal = {Journal of Asian Earth Sciences},
keywords = {Crystal growth rates,Deccan Traps,Folder - Large Igneous Provinces,Isotopic diffusion,LA-ICPMS,Plagioclase residence times,Sr isotope zoning,flood basalts},
