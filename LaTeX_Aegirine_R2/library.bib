journal = {Journal of Petrology},
keywords = {Folder - Peralkaline Field Area - Tenerife,Folder - Phase Equilibria,Phonolite,Teide–Pico Viejo,Tenerife,basanite,lineages},
language = {en},
mendeley-tags = {Folder - Peralkaline Field Area - Tenerife,Folder - Phase Equilibria,Phonolite,Teide–Pico Viejo,Tenerife,basanite,lineages},
month = {may},
