@article{Lustrino2016,
abstract = {The volcanic products of the late Miocene Morron de Villamayor volcano (Calatrava Volcanic Field, central Spain) are known for being one of the few outcrops of leucitites in the entire circum-Mediterranean area. These rocks are important because aragonite of mantle origin has been reported as inclusion in olivine macrocrysts. We use petrographic observations, mineral compositions, as well as oxygen and carbon isotope ratios coupled with experimental petrology to understand the origin of carbonate phase in these olivine-phyric rocks. Groundmass and macrocryst olivines range from $\delta$18OVSMOWof +4.8‰, typical of mantle olivine values, to +7.4‰, indicating contamination by sedimentary carbonate. Carbonates are characterized by heavy oxygen isotope compositions ($\delta$18OVSMOW{\textgreater}+24‰), and relatively light carbon isotopes ($\delta$13CPDB{\textless}−11‰), resembling skarn values, and distinct from typical mantle carbonatite compositions. Petrography, mineral compositions such as low Mg{\#} of clinopyroxene and biotite, low Ca{\#} and low incompatible element abundance of the carbonate, and isotopic ratios of O and C, do not support a mantle origin for the carbonate. Rather, the carbonate inclusions found in the olivine macrocrysts are interpreted as basement limestone fragments entrapped by the rising crystallizing magma. Comparison with experimental carbonatitic and silicate-carbonatitic melts indicates that low-degree partial melts of a carbonated peridotite must have a dolomitic rather than the aragonitic/calcitic composition as those found trapped in the Morron de Villamayor olivine macrocrysts.},
author = {Lustrino, Michele and Prelevi{\'{c}}, Dejan and Agostini, Samuele and Gaeta, Mario and {Di Rocco}, Tommaso and Stagno, Vincenzo and Capizzi, Luca Samuele},
doi = {10.1016/j.gca.2016.02.026},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Lustrino et al/2016 - Lustrino et al. - Ca-rich carbonates associated with ultrabasic-ultramafic melts Carbonatite or limestone xenoliths A case study.pdf:pdf},
issn = {00167037},
journal = {Geochimica et Cosmochimica Acta},
pages = {477--497},
title = {{Ca-rich carbonates associated with ultrabasic-ultramafic melts: Carbonatite or limestone xenoliths? A case study from the late Miocene Morron de Villamayor volcano (Calatrava Volcanic Field, central Spain)}},
volume = {185},
year = {2016}
