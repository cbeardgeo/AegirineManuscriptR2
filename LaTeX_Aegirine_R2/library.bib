doi = {10.1093/petrology/egs033},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/And{\'{u}}jar, Scaillet/2012 - And{\'{u}}jar, Scaillet - Experimental constraints on parameters controlling the difference in the eruptive dynamics of phonolitic magm.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/And{\'{u}}jar, Scaillet/2012 - And{\'{u}}jar, Scaillet - Experimental constraints on parameters controlling the difference in the eruptive dynamics of phonolitic mag.html:html},
isbn = {0022-3530},
