journal = {Earth-Science Reviews},
keywords = {Carbon dioxide,Halogens,Magmatic volatiles,Solubility,Sulfur,Water},
month = {sep},
number = {3-4},
pages = {298--324},
publisher = {Elsevier},
title = {{Fluid saturation and volatile partitioning between melts and hydrous fluids in crustal magmatic systems: The contribution of experimental measurements and solubility models}},
