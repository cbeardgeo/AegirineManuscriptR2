issn = {0084-6597},
journal = {Annual Review of Earth and Planetary Sciences},
keywords = {dike,graben,mantle plume,plate tectonics,rift},
mendeley-tags = {dike,graben,mantle plume,plate tectonics,rift},
month = {may},
