pages = {75--112},
shorttitle = {Major, trace element and Nd–Sr–Pb–O–He–Ar isotope },
title = {{Major, trace element and Nd–Sr–Pb–O–He–Ar isotope signatures of shield stage lavas from the central and western Canary Islands: Insights into mantle and crustal processes}},
