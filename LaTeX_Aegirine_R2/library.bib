file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Berryman et al/2016 - Berryman et al. - Influence of the X-site composition on tourmaline's crystal structure investigation of synthetic K‐dravite, dra.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Berryman et al/2016 - Berryman et al. - Influence of the X-site composition on tourmaline's crystal structure investigation of synthetic K‐dravite, dr.html:html},
issn = {0342-1791, 1432-2021},
journal = {Physics and Chemistry of Minerals},
