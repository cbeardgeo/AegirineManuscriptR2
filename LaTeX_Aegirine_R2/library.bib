@article{McCreath2013,
abstract = {Pyrochlore minerals are common accessory phases in the syenite and nepheline syenite rocks of the Motzfeldt alkaline center, Gardar$\backslash$nProvince, South Greenland, and are of important economic interest due to$\backslash$ntheir exceptional ability to host various high field strength elements.$\backslash$nIn this study pyrochlore from two distinct intrusive units within the$\backslash$nMotzfeldt Center have been studied. The largely homogeneous syenite and$\backslash$nnepheline syenite rocks of the Flinks Dal formation host oscillatory and$\backslash$nsector-zoned pyrochlore, which have experienced minor alteration in the$\backslash$npresence of high-temperature, dominantly juvenile magmatic fluids.$\backslash$nAlteration is characterized by variable decrease in the Na, Ca, and F$\backslash$ncontents and addition of Sr and K, accompanied by introduction of A and$\backslash$nY site vacancies. Pyrochlore from the more heterogeneous and intensely$\backslash$naltered syenite rocks of the Motzfeldt So Formation are pervasively$\backslash$naltered through interaction with lower temperature evolved hydrothermal$\backslash$nfluids. During this phase of alteration cations are preferentially$\backslash$nleached from the A and Y sites, accompanied by progressively increasing$\backslash$nhydration. In both formations the proportion of B-site cations remains$\backslash$nunaltered regardless of the degree of alteration. The alteration encoded$\backslash$nin the pyrochlores of the Motzfeldt Center records the different$\backslash$nhydrothermal conditions the two formations experienced during the$\backslash$nsubsolidus, showing how evolution of fluids within one intrusive unit$\backslash$ncan vary both spatially and temporally.},
author = {McCreath, Jamie A. and Finch, Adrian A. and Herd, Donald A. and Armour-Brown, Ashlyn},
doi = {10.2138/am.2013.4068},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/McCreath et al/2013 - McCreath et al. - Geochemistry of pyrochlore minerals from the Motzfeldt center, South Greenland The mineralogy of a syenite-host.pdf:pdf},
issn = {0003004X},
journal = {American Mineralogist},
keywords = {Alkaline rocks,Gardar province,High field strength elements,Hydrothermal alteration,Tantalum},
number = {2-3},
pages = {426--438},
title = {{Geochemistry of pyrochlore minerals from the Motzfeldt Center, South Greenland: The mineralogy of a syenite-hosted Ta, Nb deposit}},
volume = {98},
year = {2013}
}
@article{Civetta1988,
abstract = {Six silicic eruptive cycles have been recognized in the last 50 ka at Pantelleria. The products of each cycle exhibit a compositional variation from pantellerite to less peralkaline rhyolite or to trachyte. The relationships between the range of chemical variation, the erupted volume and the time of eruptions, allow us to estimate an average differentiation rate of 5{\%} crystal fractionation per 1000 years and a constant long-term rate of magma discharge of 0.1 km3 per 1000 years. Pressure increase in the magma chamber caused by the addition of new magma, accumulation of highly-differentiated, volatile-rich magma in the roof zone and a concomitant build-up of a vapour phase, is postulated as a possible triggering mechanism for eruptions.},
