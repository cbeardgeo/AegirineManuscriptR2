issn = {00223530},
journal = {Journal of Petrology},
keywords = {Fluorine,Granite,Liquid immiscibility,Rhyolite,Silicate melt},
number = {4},
pages = {785--806},
title = {{Liquidus equilibria in the system K2O-Na2O-A12O3-SiO2-F2O-1 -H2O to 100 MPa: I. Silicate-fluoride liquid immiscibility in anhydrous systems}},
