month = {jan},
number = {1–4},
pages = {14--48},
series = {Links Between Ophiolites and LIPs in Earth History},
title = {{Geochemical fingerprinting of oceanic basalts with applications to ophiolite classification and the search for Archean oceanic crust}},
url = {http://www.sciencedirect.com/science/article/pii/S0024493707001375 http://www.sciencedirect.com/science/article/pii/S0024493707001375/pdf?md5=3ed80be12ac67fc10e3e64ac28f46869{\&}pid=1-s2.0-S0024493707001375-main.pdf},
volume = {100},
