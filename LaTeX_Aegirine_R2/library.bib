title = {{F and Cl diffusion in phonolitic melts: Influence of the Na/K ratio}},
url = {http://www.sciencedirect.com/science/article/pii/S0009254108003744 http://www.sciencedirect.com/science/article/pii/S0009254108003744/pdfft?md5=c828b5ca78ab8066cb315062cf56d32f{\&}pid=1-s2.0-S0009254108003744-main.pdf},
