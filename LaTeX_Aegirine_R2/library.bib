number = {2–3},
pages = {175--184},
title = {{Cl effects on structure of Zr and Ti alkali aluminosilicate glasses}},
url = {http://www.sciencedirect.com/science/article/pii/S0022309398008175{\%}5Cnhttp://www.sciencedirect.com/science/article/pii/S0022309398008175/pdfft?md5=adb843a2d01fa5fbcf20046749530579{\&}pid=1-s2.0-S0022309398008175-main.pdf},
