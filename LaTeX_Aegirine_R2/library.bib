url = {http://geology.gsapubs.org/content/25/4/299.short http://rsta.royalsocietypublishing.org/content/297/1431/409 http://rsta.royalsocietypublishing.org/content/297/1431/409.full.pdf http://rsta.royalsocietypublishing.org/content/297/1431/409.short},
