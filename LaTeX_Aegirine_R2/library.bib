journal = {The Canadian Mineralogist},
keywords = {California,Folder - Peralkaline Field Area,Mountain Pass,Sulphide Queen,carbonatite,enriched mantle,rare-earth mineralization,ultrapotassic alkaline rocks},
language = {en},
mendeley-tags = {California,Folder - Peralkaline Field Area,Mountain Pass,Sulphide Queen,carbonatite,enriched mantle,rare-earth mineralization,ultrapotassic alkaline rocks},
month = {aug},
