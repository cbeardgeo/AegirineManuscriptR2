doi = {10.2113/econgeo.107.5.1043},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Schneider, Bachtel, Schmitt/2012 - Schneider, Bachtel, Schmitt - Zircon Alteration in Wall Rock of Pamour and Hoyle Pond Au Deposits, Abitibi Greenstone Belt Constr.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Schneider, Bachtel, Schmitt/2012 - Schneider, Bachtel, Schmitt - Zircon Alteration in Wall Rock of Pamour and Hoyle Pond Au Deposits, Abitibi Greenstone Belt Const.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Schneider, Bachtel, Schmitt/2012 - Schneider, Bachtel, Schmitt - Zircon Alteration in Wall Rock of Pamour and Hoyle Pond Au Deposits, Abitibi Greenstone Belt Constr.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Schneider, Bachtel, Schmitt/2012 - Schneider, Bachtel, Schmitt - Zircon Alteration in Wall Rock of Pamour and Hoyle Pond Au Deposits, Abitibi Greenstone Belt Const.html:html},
