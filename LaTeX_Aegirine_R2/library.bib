doi = {10.1144/jgs2013-118},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Larsen et al/2014 - Larsen et al. - Eocene to Miocene igneous activity in NE Greenland northward younging of magmatism along the East Greenland margi.pdf:pdf},
