file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Rainbird et al/1994 - Rainbird et al. - The Shaler Supergroup and revision of Neoproterozoic stratigraphy in Amundsen Basin, Northwest Territories(2).pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Rainbird et al/1994 - Rainbird et al. - The Shaler Supergroup and revision of Neoproterozoic stratigraphy in Amundsen Basin, Northwest Territories.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Rainbird et al/1994 - Rainbird et al. - The Shaler Supergroup and revision of Neoproterozoic stratigraphy in Amundsen Basin, Northwest Territories.html:html},
