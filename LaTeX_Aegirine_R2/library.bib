month = {mar},
number = {5},
pages = {1415--1426},
shorttitle = {Core formation and the oxidation state of the Eart},
title = {{Core formation and the oxidation state of the Earth: Additional constraints from Nb, V and Cr partitioning}},
url = {http://www.sciencedirect.com/science/article/pii/S0016703708000173 http://www.sciencedirect.com/science/article/pii/S0016703708000173/pdfft?md5=ce86f997f51609b5add475d79a026bc8{\&}pid=1-s2.0-S0016703708000173-main.pdf},
