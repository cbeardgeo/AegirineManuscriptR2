title = {{The Mountain Pass Rare-Earth Carbonatite and Associated Ultrapotassic Rocks, California}},
url = {http://canmin.geoscienceworld.org/content/46/4/779 http://canmin.geoscienceworld.org/content/46/4/779.full.pdf http://canmin.geoscienceworld.org/content/46/4/779.short},
