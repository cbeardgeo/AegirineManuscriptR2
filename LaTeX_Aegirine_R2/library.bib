doi = {10.1093/petrology/3.1.82},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Eugster, Wones/1962 - Eugster, Wones - Stability relations of the ferruginous biotite, annite.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Eugster, Wones/1962 - Eugster, Wones - Stability relations of the ferruginous biotite, annite.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Eugster, Wones/1962 - Eugster, Wones - Stability relations of the ferruginous biotite, annite.pdf:pdf},
