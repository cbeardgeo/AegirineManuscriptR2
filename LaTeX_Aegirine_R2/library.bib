journal = {Journal of Petrology},
keywords = {d u c t,i n t ro,i o n,magmatic conditions that affect,nated by changes in,or termi-,sustained,the,triggered,volcanic eruptions are often},
number = {3},
pages = {671--684},
title = {{Monitoring the Magmas Fuelling Volcanic Eruptions in Near-real-time Using X-ray Micro-computed Tomography}},
url = {http://petrology.oxfordjournals.org/content/55/3/671.full},
volume = {55},
year = {2014}
