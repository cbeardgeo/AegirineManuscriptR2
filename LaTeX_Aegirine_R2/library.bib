doi = {10.1046/j.1365-3121.1997.d01-15.x},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Kl{\"{u}}gel, Hansteen, Schmincke/1997 - Kl{\"{u}}gel, Hansteen, Schmincke - Rates of magma ascent and depths of magma reservoirs beneath La Palma (Canary Islands).pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Kl{\"{u}}gel, Hansteen, Schmincke/1997 - Kl{\"{u}}gel, Hansteen, Schmincke - Rates of magma ascent and depths of magma reservoirs beneath La Palma (Canary Islands).html:html},
