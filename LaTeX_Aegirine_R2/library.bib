file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Coggon et al/2012 - Coggon et al. - The 190Pt- 186Os decay system applied to dating platinum-group element mineralization of the Bushveld Complex, So.pdf:pdf},
isbn = {0009-2541},
issn = {00092541},
journal = {Chemical Geology},
keywords = {Bushveld,Dating,Geochronology,Isotopes,PGM,Pt-Os},
pages = {48--60},
