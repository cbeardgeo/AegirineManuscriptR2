keywords = {Eudialyte,Igneous layering,Il??maussaq complex,Kakortokites,Mat-formation},
pages = {295--309},
publisher = {Elsevier B.V.},
title = {{Crystal mat-formation as an igneous layering-forming process: Textural and geochemical evidence from the 'lower layered' nepheline syenite sequence of the Il??maussaq complex, South Greenland}},
