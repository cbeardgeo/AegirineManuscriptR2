doi = {10.1016/0016-7037(84)90352-1},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Chauvel, Jahn/1984 - Chauvel, Jahn - NdSr isotope and REE geochemistry of alkali basalts from the Massif Central, France.pdf:pdf},
isbn = {0016-7037},
issn = {00167037},
journal = {Geochimica et Cosmochimica Acta},
number = {1},
pages = {93--110},
title = {{NdSr isotope and REE geochemistry of alkali basalts from the Massif Central, France}},
volume = {48},
year = {1984}
}
@article{Tornos2016,
abstract = {We propose an integrated model that explains the magmatic and hydrothermal features of the unique El Laco iron deposit that is located in a Pliocene–Pleistocene volcano of the Chilean Andes. (Sub)volcanic crystallization of an iron-rich melt as massive magnetite promoted the exsolution of a small volume of a hydrosaline melt and of large amounts of vapor that led to the formation of an alkali-calcic hydrothermal assemblage replacing the host andesite; this assemblage is capped and overprinted by a large zone of acid-sulfate steam-heated alteration forming as a whole a protracted shallow-level magmatic-hydrothermal system. Oxygen isotopic data for the massive magnetite ($\delta$18O: 4.3‰–5.0‰) and the alkali-calcic altered rock (diopside $\delta$18O: 7.2‰–8.7‰; magnetite $\delta$18O: 4.4‰–6.7‰) suggest that these rocks are genetically related to the host andesite ($\delta$18O: 7.4‰–9.6‰). The estimated temperature of the mineral assemblage ({\textgreater}∼900 °C) may be the highest recorded in hydrothermal systems, is attributed to exsolution of fluids from the crystallizing iron-rich melts, and is considered unlikely to reflect the convection of surficial fluids.},
author = {Tornos, Fernando and Velasco, Francisco and Hanchar, John M.},
doi = {10.1130/G37705.1},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Tornos, Velasco, Hanchar/2016 - Tornos, Velasco, Hanchar - Iron-rich melts, magmatic magnetite, and superheated hydrothermal systems The El Laco deposit, Chile.pdf:pdf},
issn = {19432682},
journal = {Geology},
number = {6},
pages = {427--430},
title = {{Iron-rich melts, magmatic magnetite, and superheated hydrothermal systems: The El Laco deposit, Chile}},
volume = {44},
year = {2016}
}
