shorttitle = {Highly heterogeneous Late Mesozoic lithospheric ma},
title = {{Highly heterogeneous Late Mesozoic lithospheric mantle beneath the North China Craton: evidence from Sr–Nd–Pb isotopic systematics of mafic igneous rocks}},
