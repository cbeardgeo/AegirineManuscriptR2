file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Wilson, Zeh, Gerdes/2017 - Wilson, Zeh, Gerdes - In situ Sr isotopes in plagioclase and trace element systematics in the lowest part of the eastern Bushv(2).pdf:pdf},
issn = {14602415},
journal = {Journal of Petrology},
keywords = {Bushveld Complex,Crustal contamination,In situ Sr isotopes in plagioclase,Incompatible trace elements,Magma reservoirs,SCLM},
number = {2},
pages = {327--360},
title = {{In situ Sr isotopes in plagioclase and trace element systematics in the lowest part of the eastern Bushveld Complex: Dynamic processes in an evolving magma chamber}},
volume = {58},
