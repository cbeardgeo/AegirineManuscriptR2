title = {{Short-duration contact metamorphism of calcareous sedimentary rocks by Neoproterozoic Franklin gabbro sills and dikes on Victoria Island, Canada}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/jmg.12015/abstract},
