journal = {Journal of the Geological Society},
number = {4},
pages = {539--553},
title = {{Eocene to Miocene igneous activity in NE Greenland: northward younging of magmatism along the East Greenland margin}},
url = {http://jgs.lyellcollection.org/cgi/doi/10.1144/jgs2013-118},
volume = {171},
year = {2014}
}
