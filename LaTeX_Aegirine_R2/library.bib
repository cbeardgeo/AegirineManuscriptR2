journal = {Chemical Geology},
keywords = {Canary Islands,Mantle plume,Radiogenic isotopes,noble gases,trace elements},
mendeley-tags = {Canary Islands,Mantle plume,Radiogenic isotopes,noble gases,trace elements},
month = {sep},
