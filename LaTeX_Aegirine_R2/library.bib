keywords = {Asthenospheric vs. lithospheric melting,Dynamic melting,Folder - Peralkaline Field Area - Old - Pantelleri,Passive mantle upwelling,Sicily Channel rift zone,U-series disequilibria,Within-plate continental magmatism},
mendeley-tags = {Asthenospheric vs. lithospheric melting,Dynamic melting,Folder - Peralkaline Field Area - Old - Pantelleri,Passive mantle upwelling,Sicily Channel rift zone,U-series disequilibria,Within-plate continental magmatism},
shorttitle = {Mantle melting in within-plate continental setting},
title = {{Mantle melting in within-plate continental settings: Sr–Nd–Pb and U-series isotope constraints in alkali basalts from the Sicily Channel (Pantelleria and Linosa Islands, Southern Italy)}},
