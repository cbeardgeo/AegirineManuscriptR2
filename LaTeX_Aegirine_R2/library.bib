journal = {Chemical Geology},
keywords = {Agpaitic syenites,Eudialyte,Laser ablation,Sr, Nd and Hf isotopic ratios,U-Pb ages},
number = {1-2},
pages = {8--34},
publisher = {Elsevier B.V.},
title = {{In situ U-Pb, Sr, Nd and Hf isotopic analysis of eudialyte by LA-(MC)-ICP-MS}},
url = {http://dx.doi.org/10.1016/j.chemgeo.2010.02.007},
