title = {{The Quaternary pyroclastic succession of southeast Tenerife, Canary Islands: explosive eruptions, related caldera subsidence, and sector collapse}},
url = {http://journals.cambridge.org/article{\_}S0016756802007252 http://journals.cambridge.org/action/displayAbstract?fromPage=online{\&}aid=152363{\&}fileId=S0016756802007252 http://journals.cambridge.org/action/displayFulltext?type=1{\&}fid=152364{\&}jid=GEO{\&}volumeId=140{\&}is},
volume = {140},
year = {2003}
}
@article{Sonnenthal1998,
abstract = {During the middle stages of crystallization of the Skaergaard Layered Series large numbers of blocks became detached from the Upper Border Series and settled into the mush of crystals on the floor. It has been recognized for some time that these blocks now have compositions and textures that differ markedly from those of the units from which they came. They tend to be more plagioclase rich and seem to have lost mafic components to the surrounding gabbro. Numerical simulations coupling crystallization, melting, and heat and mass transfer for a multicomponent system show how the blocks reacted with the mush in which they were emplaced. Enhanced cooling and crystallization of a compositionally stratified mush adjacent to the blocks resulted in patterns of melt compositions similar to those of layering around the blocks. Volume changes during crystallization and melting induced convection of the interstitial melt leading to changes in the bulk compositions of the blocks and the surrounding mush. Inhomogeneities such as inclusions are likely to facilitate the onset of compositional convection in a chemically stratified solidification zone.},
author = {Sonnenthal, Eric L. and McBirney, Alexander R.},
doi = {10.1093/petroj/39.4.633},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Sonnenthal, McBirney/1998 - Sonnenthal, McBirney - The Skaergaard Layered Series. Part IV. Reaction–Transport Simulations of Foundered Blocks.pdf:pdf},
