@article{Corgne2005CMP,
abstract = {We have determined the partition coefficients of a large number of trace elements between CaTiO3 perovskite and anhydrous silicate melts at atmospheric pressure and 3 GPa. Determination of the concentration limits of Henry's law behaviour in the CaO-Al2O3–SiO2–TiO2 system reveals that the incorporation of rare earth elements (REE) and tetravalent large ion lithophile elements (LILE4+ such as U and Th) at the Ca-site of CaTiO3 perovskite occurs with charge compensation through Ca-vacancy formation rather than by coupled substitution of Al for Ti. When melt composition is varied, we find that partition coefficients for REE and Th are strong functions of the CaO content of the melt. The observed trends are in excellent agreement with those predicted from the Ca-vacancy model. Given that they adopt the same crystal structure and have similar trace element partitioning behaviour, CaTiO3 perovskite and the deep mantle phase CaSiO3 perovskite can be considered analogous to one another. When the analogy is pursued in detail, we find that partitioning into both phases follows the composition-dependence predicted by the Ca-vacancy model. Thus, substitution of REE, U4+ and Th into CaSiO3 in the lower mantle also occurs with Ca-vacancy formation to balance charge. Furthermore when 2+, 3+ and 4+ partition coefficients for both phases are plotted as functions of CaO melt content, the trends for CaSiO3 and CaTiO3 appear to be continuous. This surprising result means that partitioning into Ca-perovskite is independent of pressure and temperature and also of whether or not the host is CaSiO3 or CaTiO3. One implication is that CaSiO3 crystallising from a peridotitic magma ocean may have partition coefficients for Th and U up to about 400. Crystallisation and sequestration of as little as 0.25 volume{\%} of this phase in the lower mantle early in earth history would make a significant contribution to current mantle heat production.},
author = {Corgne, Alexandre and Wood, Bernard J.},
doi = {10.1007/s00410-004-0638-3},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Caricchi et al/2014 - Caricchi et al. - Frequency and magnitude of volcanic eruptions controlled by magma injection and buoyancy.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Caricchi et al/2014 - Caricchi et al. - Frequency and magnitude of volcanic eruptions controlled by magma injection and buoyancy.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Corgne, Wood/2005 - Corgne, Wood - Trace element partitioning and substitution mechanisms in calcium perovskites.pdf:pdf},
issn = {0010-7999, 1432-0967},
journal = {Contributions to Mineralogy and Petrology},
keywords = {Folder - Trace element partitioning},
language = {en},
mendeley-tags = {Folder - Trace element partitioning},
month = {mar},
number = {1},
pages = {85--97},
title = {{Trace element partitioning and substitution mechanisms in calcium perovskites}},
url = {http://link.springer.com/article/10.1007/s00410-004-0638-3 http://link.springer.com/article/10.1007/s00410-004-0638-3{\#}page-1 http://link.springer.com/content/pdf/10.1007{\%}2Fs00410-004-0638-3.pdf},
volume = {149},
year = {2005}
}
@article{Hutchison2016b,
abstract = {The silicic peralkaline volcanoes of the East African Rift are some of the least studied volcanoes on Earth. Here we bring together new constraints from fieldwork, remote sensing, geochronology and geochemistry to present the first detailed account of the eruptive history of Aluto, a restless silicic volcano located in a densely populated section of the Main Ethiopian Rift. Prior to the growth of the Aluto volcanic complex (before 500 ka) the region was characterized by a significant period of fault development and mafic fissure eruptions. The earliest volcanism at Aluto built up a trachytic complex over 8 km in diameter. Aluto then underwent large-volume ignimbrite eruptions at 316 ± 19 ka and 306 ± 12 ka developing a {\~{}} 42 km2 collapse structure. After a hiatus of {\~{}} 250 ka, a phase of post-caldera volcanism initiated at 55 ± 19 ka and the most recent eruption of Aluto has a radiocarbon age of 0.40 ± 0.05 cal. ka BP. During this post-caldera phase highly-evolved peralkaline rhyolite lavas, ignimbrites and pumice fall deposits have erupted from vents across the complex. Geochemical modelling is consistent with rhyolite genesis from protracted fractionation ({\textgreater} 80{\%}) of basalt that is compositionally similar to rift-related basalts found east of the complex. Based on the style and volume of recent eruptions we suggest that silicic eruptions occur at an average rate of 1 per 1000 years, and that future eruptions of Aluto will involve explosive emplacement of localised pumice cones and effusive obsidian coulees of volumes in the range 1–100 × 106 m3.},
author = {Hutchison, William and Pyle, David M. and Mather, Tamsin A. and Yirgu, Gezahegn and Biggs, Juliet and Cohen, Benjamin E. and Barfod, Dan N. and Lewi, Elias},
doi = {10.1016/j.jvolgeores.2016.09.010},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Hutchison et al/2016 - Hutchison et al. - The eruptive history and magmatic evolution of Aluto volcano new insights into silicic peralkaline volcanism i.pdf:pdf},
issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {Main Ethiopian Rift,eruptive history,geothermal resources,peralkaline,silicic volcanism,volcanic hazard},
pages = {9--33},
publisher = {The Authors},
title = {{The eruptive history and magmatic evolution of Aluto volcano: new insights into silicic peralkaline volcanism in the Ethiopian rift}},
url = {http://dx.doi.org/10.1016/j.jvolgeores.2016.09.010},
volume = {328},
year = {2016}
}
@article{Estrade2014,
abstract = {The almost unknown Ambohimirahavavy ring complex in the Cenozoic alkaline province of northwestern Madagascar has recently attracted considerable interest because of the discovery of important rare-metal mineralization. The complex consists of arc-shaped bodies made up of silica-under- and -oversaturated syenites and extremely evolved peralkaline granitic dykes, as well as several mafic to felsic volcanic units, including basalt, phonolite and trachyte, all of which have an alkaline affinity. Uranium-lead zircon ages of 24.2. ±. 0.6. Ma and 23.5. ±. 6.8. Ma have been obtained for nepheline syenites and peralkaline granitic dykes, respectively, which, together with field data and ages of neighboring complexes, support emplacement controlled by regional lithospheric structures, rather than an evolving hot spot.Whole-rock major and trace-element and Sr-Nd isotopic data for the mafic suite suggest that the parental melt of this complex was generated by low degrees of melting of a metasomatized mantle source with residual amphibole. Fractional crystallization of this alkali basaltic melt likely produced the silica-undersaturated suite. We propose that the silica-oversaturated suite evolved from the undersaturated melt after contamination of the latter by crustal material. Further evolution to peralkaline compositions in both suites is attributed mainly to plagioclase and alkali feldspar segregation.Nepheline and feldspar compositions, as well as considerations of mineral equilibria among mafic silicates and Fe-Ti oxide minerals indicate crystallization temperatures of 1000 to 700. °C and an oxygen fugacity of 0.4 to 0.8. log units below the fayalite-magnetite-quartz (FMQ) buffer at 1. kbar for the silica-undersaturated melt, and temperatures of 860 to 570. °C and an oxygen fugacity of 1.5 to 3.8. log units below FMQ for the oversaturated syenitic melt. The undersaturated melt evolved towards a more peralkaline composition. Crystallization of arfvedsonite plus aegirine further reduced the melt the evolution of which ended with fluid exsolution. At late stages of crystallization, the oversaturated melt departed from the reducing trend of the undersaturated melt, evolving towards high oxygen fugacity. Very late exsolution of the fluid permitted concentration of the HFSE in the last stages of magmatic evolution with local production of low-temperature pegmatitic phases extremely enriched in these elements. {\textcopyright} 2014 Elsevier B.V.},
author = {Estrade, Guillaume and B{\'{e}}ziat, Didier and Salvi, Stefano and Tiepolo, Massimo and Paquette, Jean Louis and Rakotovao, Soatsitohaina},
doi = {10.1016/j.lithos.2014.08.008},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Estrade et al/2014 - Estrade et al. - Unusual evolution of silica-under- and -oversaturated alkaline rocks in the Cenozoic Ambohimirahavavy Complex (M.pdf:pdf},
isbn = {0024-4937},
issn = {00244937},
journal = {Lithos},
keywords = {Alkaline complex,Cenozoic magmatism,Madagascar,Oxygen fugacity,Peralkaline,U-Pb dating},
number = {1},
pages = {361--383},
publisher = {Elsevier B.V.},
title = {{Unusual evolution of silica-under- and -oversaturated alkaline rocks in the Cenozoic Ambohimirahavavy Complex (Madagascar): Mineralogical and geochemical evidence}},
url = {http://dx.doi.org/10.1016/j.lithos.2014.08.008},
volume = {206-207},
year = {2014}
}
@article{Simakin2004,
abstract = {Plagioclase crystallization from a hydrous hawaiitic melt was studied by various methods. The temperature of metastable liquidus extension (temperature of plagioclase equilibrium with the melt of the given composition) was estimated as 1070 ± 5°C at PH2O = 600 bar and f O2 ≈ NNO. This is 20-25°C higher than the value calculated by the MELTS code. Plagioclase growth rate at an undercooling of 10-15° is about 1.5 × 10-5 cm/s. It was shown that iron content in plagioclase increases with increasing undercooling, and lath-shaped quench plagioclase crystals contain up to 2.0-3.5 wt {\%} FeO. Iron content in recrystallized plagioclase is about 0.5 wt {\%}, which is close to that of the intrusive plagioclase that was used as a seed. The experimental plagioclase crystals that grew during degassing of water-saturated hawaiitic melt are similar to natural plagioclase from the lavas of Mount Etna Volcano with respect to iron content (0.5-0.8 wt {\%}) and albite mole fraction. The crystal size distribution of plagioclase in the hawaiites of Etna shows an exponential segment for grain sizes about L {\textless} 0.6 mm. According to our experiments, this segment could correspond to crystallization coupled with degassing. The crystals could attain the critical size during the ascent of hawaiitic magma from the level of water saturation to the surface within 3-5 h. An increase in the population density toward small plagioclase crystal sizes could correspond to the increasing rate of nucleation at relatively low overcooling due to the melt degassing. The analysis of the IR spectra of hawaiitic glasses demonstrated that dissolution of more than 1 wt {\%} water results in melt depletion in feldspar-like structural units (four-membered aluminosilicate rings).},
author = {Simakin, A G and Salova, T P},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Simakin, Salova/2004 - Simakin, Salova - Plagioclase Crystallization from a Hawaiitic Melt in Experiments and in a Volcanic Conduit.pdf:pdf},
issn = {08695911 (ISSN)},
journal = {Petrology},
keywords = {Computer simulation,Crystallization,Degassing,Interpolation,Magma,Microcrystals,Petrology,Pressure effects,Solubility,Tectonics,Volcanic rocks},
number = {1},
pages = {82--92},
title = {{Plagioclase Crystallization from a Hawaiitic Melt in Experiments and in a Volcanic Conduit}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-1142291619{\&}partnerID=40{\&}md5=bdbb8d1357f5717f8f4e5640fc1c55cf},
volume = {12},
year = {2004}
}
@article{Prowatke2005,
abstract = {Isobaric and isothermal experiments were performed to investigate the effect of melt composition on the partitioning of trace elements between titanite (CaTiSiO5) and a range of different silicate melts. Titanite-melt partition coefficients for 18 trace elements were determined by secondary ion mass spectrometry (SIMS) analyses of experimental run products. The partition coefficients for the rare earth elements and for Th, Nb, and Ta reveal a strong influence of melt composition on partition coefficients, whereas partition coefficients for other studied monovalent, divalent and most quadrivalent (i.e., Zr, Hf) cations are not significantly affected by melt composition. The present data show that the influence of melt composition may not be neglected when modelling trace element partitioning. It is argued that it is mainly the change of coordination number and the regularity of the coordination space of trace elements in the melt structure that controls partition coefficients in our experiments. Furthermore, our data also show that the substitution mechanism by which trace elements are incorporated into titanite crystals may be of additional importance in this context.},
author = {Prowatke, Stefan and Klemme, Stephan},
doi = {10.1016/j.gca.2004.06.037},
