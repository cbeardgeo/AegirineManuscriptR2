doi = {10.1016/0024-4937(94)90010-8},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Bailey, Gwozdz/1994 - Bailey, Gwozdz - Li distribution in aegirine lujavrite, Ilmaussaq alkaline intrusion, South Greenland role of cumulus and post-cu.pdf:pdf},
issn = {00244937},
journal = {Lithos},
number = {3-4},
pages = {207--225},
title = {{Li distribution in aegirine lujavrite, Il??maussaq alkaline intrusion, South Greenland: role of cumulus and post-cumulus processes}},
volume = {31},
year = {1994}
}
@book{Mahoney1997,
