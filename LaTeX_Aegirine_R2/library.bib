doi = {10.1038/ngeo2279},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Nishikawa, Ide/2014 - Nishikawa, Ide - Earthquake size distribution in subduction zones linked to slab buoyancy(2).pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Nishikawa, Ide/2014 - Nishikawa, Ide - Earthquake size distribution in subduction zones linked to slab buoyancy(2).html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Nishikawa, Ide/2014 - Nishikawa, Ide - Earthquake size distribution in subduction zones linked to slab buoyancy.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Nishikawa, Ide/2014 - Nishikawa, Ide - Earthquake size distribution in subduction zones linked to slab buoyancy.html:html},
