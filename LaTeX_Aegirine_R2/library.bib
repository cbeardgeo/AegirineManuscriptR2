issn = {1525-1314},
journal = {Journal of Metamorphic Geology},
keywords = {Folder - EPS Thermo,P–T,thermodynamic data.},
language = {en},
mendeley-tags = {Folder - EPS Thermo,P–T,thermodynamic data.},
month = {may},
number = {3},
pages = {309--343},
title = {{An internally consistent thermodynamic data set for phases of petrological interest}},
