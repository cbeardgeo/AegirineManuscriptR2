number = {B10},
pages = {8540--8552},
shorttitle = {A combined rapid-quench and H2-membrane setup for},
title = {{Pyroclastic rocks and calderas associated with strongly peralkaline magmatism}},
url = {http://onlinelibrary.wiley.com/doi/10.1029/JB089iB10p08540/abstract},
volume = {89},
year = {1984}
}
@article{Girard2010,
abstract = {The Central Plateau Member rhyolites have been erupted between 173 and 70 ka and are the youngest Yellowstone intracaldera rhyolites. They mostly comprise very voluminous lava flows totaling {\~{}}600 km3 in volume. Their eruptive vents define two NNW-trending lineaments which are aligned with regional faults. We present new whole rock, glass, and mineral analyses and propose a petrogenetic and volcano-tectonic model for these rhyolites. At a caldera-wide scale, there is a temporal enrichment in elements such as Nb, Y and HREE, and a depletion in Sr, Ba, and Ce/Yb. Simultaneously, clinopyroxene becomes less magnesian while Ti contents in quartz decrease. By contrast, quartz in all rhyolites is rounded and bears long glass re-entrants, suggesting heating. Based on these data and observations, we propose that the Central Plateau Member rhyolites have been generated as follows. A hydrothermally altered low-$\delta$18O rhyolitic protolith beneath the Mallard Lake Resurgent Dome in the southwestern part of the caldera started to melt at {\~{}}250 ka. Repeated heating pulses caused the melting front to expand radially, and a large crystal mush formed beneath much of the caldera. The mush was able to differentiate but not erupt due to its high crystallinity and viscosity. Further inputs of heat and silicic magma in this mush increased the degree of melting, forming crystal-poor magma batches which erupted a few hundred to a few thousand years later through regional faults to form the Central Plateau Member rhyolites.},
author = {Girard, Guillaume and Stix, John},
doi = {10.1007/s00410-009-0487-1},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Petford/2009 - Petford - Emplacement-related layering in magma slurries.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Girard, Stix/2010 - Girard, Stix - Rapid extraction of discrete magma batches from a large differentiating magma chamber the Central Plateau Member r.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Girard, Stix/2010 - Girard, Stix - Rapid extraction of discrete magma batches from a large differentiating magma chamber the Central Plateau Member.html:html},
issn = {0010-7999, 1432-0967},
journal = {Contributions to Mineralogy and Petrology},
keywords = {Crystal mush,Crystal residence times,Faulting,Folder - Physical Volcanology,Geology,Intraplate volcanism,Low-$\delta$18O magmas,Mineral Resources,Mineralogy},
language = {en},
mendeley-tags = {Crystal mush,Crystal residence times,Faulting,Folder - Physical Volcanology,Geology,Intraplate volcanism,Low-$\delta$18O magmas,Mineral Resources,Mineralogy},
month = {sep},
number = {3},
pages = {441--465},
shorttitle = {Rapid extraction of discrete magma batches from a },
title = {{Rapid extraction of discrete magma batches from a large differentiating magma chamber: the Central Plateau Member rhyolites, Yellowstone Caldera, Wyoming}},
url = {http://link.springer.com/article/10.1007/s00410-009-0487-1 http://link.springer.com/article/10.1007{\%}2Fs00410-009-0487-1?LI=true http://link.springer.com/content/pdf/10.1007{\%}2Fs00410-009-0487-1},
