number = {4},
pages = {1--30},
title = {{Trace element composition of mantle end-members: Implications for recycling of oceanic and upper and lower continental crust}},
url = {http://www.agu.org/pubs/crossref/2006/2005GC001005.shtml},
volume = {7},
