journal = {Journal of the Geological Society},
keywords = {between 1350 and 1150,blaxland et,continental rifting,during a period of,formed,gardar,geochemistry,greenland,igneous intrusions,intraplate alkaline magmatism,ma ago,of sw greenland was,related to,the gardar igneous province},
number = {6},
pages = {705--714},
title = {{Long-term memory of subduction processes in the lithospheric mantle: evidence from the geochemistry of basic dykes in the Gardar Province of South Greenland}},
url = {http://jgs.lyellcollection.org/cgi/doi/10.1144/0016-764901-154},
volume = {159},
year = {2002}
}
