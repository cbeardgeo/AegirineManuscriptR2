title = {{Primary magmas and mantle temperatures}},
url = {http://eurjmin.geoscienceworld.org/content/13/3/437 http://eurjmin.geoscienceworld.org/content/13/3/437.full.pdf http://eurjmin.geoscienceworld.org/content/13/3/437.short},
