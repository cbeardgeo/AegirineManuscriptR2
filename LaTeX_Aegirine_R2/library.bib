doi = {10.1029/2009GC002382},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Keir et al/2009 - Keir et al. - Lower crustal earthquakes near the Ethiopian rift induced by magmatic processes.html:html},
