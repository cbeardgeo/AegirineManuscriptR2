file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Linnen, Keppler/1997 - Linnen, Keppler - Columbite solubility in granitic melts consequences for the enrichment and fractionation of Nb and Ta in the Ea.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Linnen, Keppler/1997 - Linnen, Keppler - Columbite solubility in granitic melts consequences for the enrichment and fractionation of Nb and Ta in the E.html:html},
issn = {0010-7999, 1432-0967},
journal = {Contributions to Mineralogy and Petrology},
language = {en},
month = {jul},
number = {2-3},
pages = {213--227},
shorttitle = {Columbite solubility in granitic melts},
title = {{Columbite solubility in granitic melts: consequences for the enrichment and fractionation of Nb and Ta in the Earth's crust}},
url = {http://link.springer.com/article/10.1007/s004100050304 http://link.springer.com/article/10.1007{\%}2Fs004100050304{\#}page-1 http://link.springer.com/content/pdf/10.1007{\%}2Fs004100050304.pdf},
volume = {128},
