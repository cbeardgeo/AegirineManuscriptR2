journal = {Annual Review of Earth and Planetary Sciences},
keywords = {a radiogenic w-isotopic composition,abstract the earth has,accretion,chronometer,compared to chon-,core formation,demonstrating that it formed,drites,half-life 9 myr,isotopes,tungsten,was extant in earth,while 182 hf},
number = {1},
pages = {531--570},
title = {{THE Hf-W ISOTOPIC SYSTEM AND THE ORIGIN OF THE EARTH AND MOON}},
