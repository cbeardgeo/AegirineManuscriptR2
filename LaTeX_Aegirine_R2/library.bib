doi = {10.1093/petrology/egq055},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Carlo et al/2010 - Carlo et al. - Phase Equilibrium Constraints on Pre-eruptive Conditions of Recent Felsic Explosive Volcanism at Pantelleria Islan.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Carlo et al/2010 - Carlo et al. - Phase Equilibrium Constraints on Pre-eruptive Conditions of Recent Felsic Explosive Volcanism at Pantelleria Isla.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Carlo et al/2010 - Carlo et al. - Phase Equilibrium Constraints on Pre-eruptive Conditions of Recent Felsic Explosive Volcanism at Pantelleria Islan.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Carlo et al/2010 - Carlo et al. - Phase Equilibrium Constraints on Pre-eruptive Conditions of Recent Felsic Explosive Volcanism at Pantelleria Isla.html:html},
