pages = {45--79},
shorttitle = {The effect of transport and boiling on Ag/Au ratio},
title = {{The effect of transport and boiling on Ag/Au ratios in hydrothermal solutions: a preliminary assessment and possible implications for the formation of epithermal precious-metal ore deposits}},
