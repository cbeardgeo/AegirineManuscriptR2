file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Crockford et al/2016 - Crockford et al. - Triple oxygen and multiple sulfur isotope constraints on the evolution of the post-Marinoan sulfur cycle.pdf:pdf},
isbn = {0012-821X},
issn = {0012821X},
journal = {Earth and Planetary Science Letters},
keywords = {Barite,Cap dolostone,Neoproterozoic,Snowball earth,Sulfur cycle,Triple oxygen},
language = {en},
month = {feb},
pages = {74--83},
title = {{Triple oxygen and multiple sulfur isotope constraints on the evolution of the post-Marinoan sulfur cycle}},
