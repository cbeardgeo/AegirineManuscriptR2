title = {{Platinum-group Elements and Microstructures of Normal Merensky Reef from Impala Platinum Mines, Bushveld Complex}},
url = {http://petrology.oxfordjournals.org/content/43/1/103 http://petrology.oxfordjournals.org/content/43/1/103.full.pdf http://petrology.oxfordjournals.org/content/43/1/103.short},
