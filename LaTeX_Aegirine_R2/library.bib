file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Albar{\`{e}}de et al/1999 - Albar{\`{e}}de et al. - New mantle convection model may reconcile conflicting evidence.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Willbold, Stracke/2006 - Willbold, Stracke - Trace element composition of mantle end-members Implications for recycling of oceanic and upper and lower con.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Albar{\`{e}}de et al/1999 - Albar{\`{e}}de et al. - New mantle convection model may reconcile conflicting evidence.html:html},
issn = {1525-2027},
journal = {Geochemistry Geophysics Geosystems},
keywords = {lower continental crust,mantle heterogeneity,marine sediments,radiogenic isotopes,subduction erosion,trace},
language = {en},
