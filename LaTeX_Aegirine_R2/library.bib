issn = {0010-7999, 1432-0967},
journal = {Contributions to Mineralogy and Petrology},
keywords = {Apatite saturation,Apatite-rich-rocks,Fluorine,Folder - Zirconosilicate Saturation,Geology,Massif type anorthosites,Mineral Resources,Mineralogy,Pressure,Silicate magmas,layered intrusions},
