journal = {Chemical Geology},
keywords = {Composition of continental crust,Loess,Shale,Tantalum,niobium},
mendeley-tags = {Composition of continental crust,Loess,Shale,Tantalum,niobium},
month = {apr},
number = {3–4},
pages = {197--213},
title = {{Tracking the budget of Nb and Ta in the continental crust}},
url = {http://www.sciencedirect.com/science/article/pii/S0009254199001734 http://www.sciencedirect.com/science/article/pii/S0009254199001734/pdf?md5=0b6ef3a8e7e4e357beb35c805fe0aeb5{\&}pid=1-s2.0-S0009254199001734-main.pdf http://www.sciencedirect.com/science/artic},
