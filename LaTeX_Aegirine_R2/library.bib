@article{Sobolev2007,
abstract = {Petrogenetic models for the origin of lamproites are evaluated using new major element, trace element, and Sr, Nd, and Pb isotope data for Holocene lamproites from the Gaussberg volcano in the East Antarctic Shield. Gaussberg lamproites exhibit very unusual Pb isotope compositions (206Pb/204Pb = 17{\textperiodcentered}44–17{\textperiodcentered}55 and 207Pb/204Pb = 15{\textperiodcentered}56–15{\textperiodcentered}63), which in common Pb isotope space plot above mantle evolution lines and to the left of the meteorite isochron. Combined with very unradiogenic Nd, such compositions are shown to be inconsistent with an origin by melting of sub-continental lithospheric mantle. Instead, a model is proposed in which late Archaean continent-derived sediment is subducted as K-hollandite and other ultra-high-pressure phases and sequestered in the Transition Zone (or lower mantle) where it is effectively isolated for 2–3 Gyr. The high 207Pb/204Pb ratio is thus inherited from ancient continent-derived sediment, and the relatively low 206Pb/204Pb ratio is the result of a single stage of U/Pb fractionation by subduction-related U loss during slab dehydration. Sr and Nd isotope ratios, and trace element characteristics (e.g. Nb/Ta ratios) are consistent with sediment subduction and dehydration-related fractionation. Similar models that use variable time of isolation of subducted sediment can be derived for all lamproites. Our interpretation of lamproite sources has important implications for ocean island basalt petrogenesis as well as the preservation of geochemically anomalous reservoirs in the mantle.},
author = {Murphy, D. T. and Collerson, K. D. and Kamber, B. S.},
doi = {10.1093/petrology/43.6.981},
file = {:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Watson, Harrison/1983 - Watson, Harrison - Zircon saturation revisited temperature and composition effects in a variety of crustal magma types.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Murphy, Collerson, Kamber/2002 - Murphy, Collerson, Kamber - Lamproites from Gaussberg, Antarctica Possible Transition Zone Melts of Archaean Subducted Sediments.pdf:pdf;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Murphy, Collerson, Kamber/2002 - Murphy, Collerson, Kamber - Lamproites from Gaussberg, Antarctica Possible Transition Zone Melts of Archaean Subducted Sediments.html:html;:C$\backslash$:/Users/cbeard/Documents/Mendeley Desktop/Watson, Harrison/1983 - Watson, Harrison - Zircon saturation revisited temperature and composition effects in a variety of crustal magma types(2).html:html},
issn = {0022-3530, 1460-2415},
